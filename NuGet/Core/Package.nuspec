      </group>
      <group targetFramework="netstandard2.0">
        <dependency id="Newtonsoft.Json" version="12.0.1" />
        <dependency id="System.ComponentModel.Annotations" version="4.5.0" />
        <dependency id="System.Reflection.Emit" version="4.3.0" />
      </group>
    </dependencies>
  </metadata>
